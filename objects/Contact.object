    <listViews>
        <fullName>Farmer_Registration</fullName>
        <columns>FULL_NAME</columns>
        <columns>ACCOUNT.NAME</columns>
        <columns>CONTACT.EMAIL</columns>
        <columns>Ag2Ag_ID__c</columns>
        <columns>Registration_Date__c</columns>
        <filterScope>Everything</filterScope>
        <filters>
            <field>Job_Function__c</field>
            <operation>contains</operation>
            <value>Farmer</value>
        </filters>
        <filters>
            <field>IsUserActive__c</field>
            <operation>equals</operation>
            <value>1</value>
        </filters>
        <label>Farmer Registration</label>
    </listViews>
