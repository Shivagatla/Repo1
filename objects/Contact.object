    <fields>
        <fullName>Business_Unit__c</fullName>
        <externalId>false</externalId>
        <formula>IF(Related_External_User__r.Corporate__c,&apos;Corporate;&apos;,&apos;&apos;) + IF(Related_External_User__r.lol__c,&apos;Dairy;&apos;,&apos;&apos;)  + 
IF(Related_External_User__r.purina__c,&apos;Purina;&apos;,&apos;&apos;) + IF(Related_External_User__r.seed__c,&apos;WinField&apos;,&apos;&apos;) +  IF( Related_External_User__r.FGI__c , &apos;FGI&apos;, &apos;&apos;)</formula>
