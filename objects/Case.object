    <validationRules>
        <fullName>EditClosedCase</fullName>
        <active>true</active>
        <description>Once Case Is Closed, Cannot Edit The Case Fields.</description>
        <errorConditionFormula>AND( PRIORVALUE( IsClosed ) = True, $Profile.Name != &quot;System Administrator&quot;)</errorConditionFormula>
        <errorMessage>Once Case Is Closed, Cannot Edit The Case Fields.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_valid_Account</fullName>
        <active>true</active>
        <description>Require the customer Contact record to be linked to a valid Account</description>
        <errorConditionFormula>AND(AND(AND(OR(Contact.Account.Name = &apos;Pending Registrations&apos;,AND(Contact.Account.RecordType.DeveloperName=&apos;System_Account&apos;,Contact.Account.Name &lt;&gt; &apos;Land O Lakes Internal&apos;)),NOT(IsClosed)), RecordType.DeveloperName &lt;&gt; &apos;EMS_Provisioning&apos;), AND(TEXT(Status) &lt;&gt; &apos;New&apos;,TEXT(Status)&lt;&gt; &apos;Reviewed and Denied&apos;))</errorConditionFormula>
        <errorDisplayField>AccountId</errorDisplayField>
        <errorMessage>Unable to save this case because the customer is not linked to a valid account. To continue, find the right account and associate it with this customer.</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>Assign_to_me</fullName>
        <availability>online</availability>
        <description>Allows a user to accept a case</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Assign To Me</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/apex.js&quot;)}

var result = sforce.connection.query(&quot;Select Id, ContactId From Case where Id = &apos;{!Case.Id}&apos; &quot;);
var records = result.getArray(&quot;records&quot;); 
var contactId = records[0].ContactId;
 
// identify the record
var o = new sforce.SObject(&quot;Case&quot;);
o.id = &quot;{!Case.Id}&quot;;


// make the field change
o.OwnerId = sforce.connection.getUserInfo().userId;
 
// save the change
sforce.connection.update([o]);

o.ContactId = contactId;
sforce.connection.update([o]);

window.location.reload();</url>
    </webLinks>
</CustomObject>
